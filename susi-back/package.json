{
  "name": "susi-backend",
  "version": "0.1.0",
  "description": "수시 전형 분석 백엔드",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "gcp-build": "nest build && npm run copy:jungsi-data",
    "build": "nest build && npm run copy:jungsi-data",
    "clean": "rimraf dist tsconfig.build.tsbuildinfo tsconfig.tsbuildinfo",
    "clean:build": "npm run clean && npm run build",
    "copy:jungsi-data": "node -e \"const fs=require('fs');const path=require('path');const src='src/modules/jungsi/calculation/data';const dst='dist/modules/jungsi/calculation/data';fs.mkdirSync(dst,{recursive:true});fs.readdirSync(src).filter(f=>f.endsWith('.json')).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dst,f)));console.log('JSON files copied to dist');\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docker:start": "docker start geobuk-postgres geobuk-redis 2>nul || echo Docker containers starting...",
    "prestart:dev": "npm run docker:start",
    "start": "nest start",
    "start:dev": "npm run copy:jungsi-data && nest start --watch",
    "start:clean": "npm run clean && npm run start:dev",
    "start:planner": "cross-env NODE_ENV=planner npm run copy:jungsi-data && cross-env NODE_ENV=planner nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env TS_NODE_PROJECT=tsconfig.paths.json node -r tsconfig-paths/register dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ormconfig.ts",
    "typeorm:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "typeorm:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ormconfig.ts",
    "typeorm:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ormconfig.ts",
    "typeorm:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ormconfig.ts",
    "validate:env": "node scripts/validate-env.js",
    "seed:admin": "ts-node scripts/seed-admin-user.ts",
    "gcloud:auth": "gcloud auth login",
    "gcloud:config": "gcloud config set project ts-back-nest-479305",
    "gcloud:deploy": "gcloud app deploy",
    "cloudrun:deploy": "gcloud run deploy geobukschool-backend --source . --region asia-northeast3 --platform managed --allow-unauthenticated --memory 1Gi --cpu 1 --min-instances 0 --max-instances 10 --port 8080",
    "cloudrun:deploy:env": "gcloud run deploy geobukschool-backend --source . --region asia-northeast3 --platform managed --allow-unauthenticated --memory 1Gi --cpu 1 --min-instances 0 --max-instances 10 --port 8080 --set-env-vars-file .env.cloudrun.yaml",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^10.0.2",
    "@opentelemetry/core": "^2.2.0",
    "@sentry/nestjs": "^10.26.0",
    "@sentry/node": "^8.17.0",
    "@sentry/profiling-node": "^10.26.0",
    "@slack/client": "^5.0.2",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^12.4.1",
    "cache-manager": "^5.5.3",
    "cache-manager-redis-yet": "^5.1.1",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "firebase-admin": "11.11.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.13.3",
    "keyv": "^4.5.4",
    "lru": "^0.1.3",
    "mysql2": "^3.10.1",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.11",
    "openai": "^6.15.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.12.0",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cheerio": "^1.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/glob": "^9.0.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lru.min": "^1.1.2",
    "playwright": "^1.57.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "xlsx": "^0.18.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
