{
  "name": "hizen-ai-be",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build:sdk": "cd packages/api && pnpm run build",
    "build:all": "nestia all && pnpm build:sdk",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "rimraf test/bin && tsc --project test/tsconfig.json && node test/bin/test/index.js",
    "prisma:generate": "prisma generate",
    "prepare": "ts-patch install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.840.0",
    "@aws-sdk/s3-request-presigner": "^3.840.0",
    "@nestia/core": "^6.0.3",
    "@nestia/e2e": "^6.0.3",
    "@nestia/fetcher": "^6.0.3",
    "@nestia/sdk": "^6.0.3",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.7.0",
    "@samchon/openapi": "^4.3.3",
    "argon2": "^0.43.0",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typia": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestia/benchmark": "^6.0.3",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.14",
    "commander": "^14.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "msw": "^2.8.4",
    "nestia": "^7.0.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.4.2",
    "prisma": "^6.7.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0",
    "typescript-transform-paths": "^3.5.5"
  },
  "packageManager": "pnpm@10.6.2"
}
