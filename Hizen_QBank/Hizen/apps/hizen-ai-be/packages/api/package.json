{
  "name": "@hizen-ai/api",
  "version": "0.1.0",
  "description": "sdk for hizen-ai service api",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./dto": {
      "default": "./lib/dto/index.js",
      "types": "./lib/dto/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:sdk && npm run compile",
    "build:sdk": "cd ../.. && npx nestia sdk && cd packages/api",
    "compile": "rimraf lib && tsc",
    "deploy": "npm run build && npm publish",
    "prepare": "ts-patch install"
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "rimraf": "^6.0.1",
    "ts-patch": "^3.3.0",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@nestia/fetcher": "^6.0.3",
    "typia": "^9.2.0"
  }
}
